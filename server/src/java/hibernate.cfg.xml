<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
  <session-factory>
    <property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
    <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
    <property name="hibernate.connection.url">jdbc:mysql://localhost:3306/diabadviser?zeroDateTimeBehavior=convertToNull</property>
    <property name="hibernate.connection.username">root</property>
    <property name="hibernate.connection.password">1234</property>
    <mapping resource="Model/DB/Hospital.hbm.xml"/>
    <mapping resource="Model/DB/Sugarrate.hbm.xml"/>
    <mapping resource="Model/DB/Paidstatus.hbm.xml"/>
    <mapping resource="Model/DB/Userloginlog.hbm.xml"/>
    <mapping resource="Model/DB/User.hbm.xml"/>
    <mapping resource="Model/DB/Doctor.hbm.xml"/>
    <mapping resource="Model/DB/Status.hbm.xml"/>
    <mapping resource="Model/DB/Tippayment.hbm.xml"/>
    <mapping resource="Model/DB/Healthtip.hbm.xml"/>
    <mapping resource="Model/DB/Patient.hbm.xml"/>
    <mapping resource="Model/DB/DoctorHasPatient.hbm.xml"/>
    <mapping resource="Model/DB/Dailytip.hbm.xml"/>
    <mapping resource="Model/DB/Maintaincharge.hbm.xml"/>
    <mapping resource="Model/DB/Bank.hbm.xml"/>
  </session-factory>
</hibernate-configuration>
